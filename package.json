{
  "name": "react-flip-toolkit",
  "version": "2.0.9",
  "description": "Configurable FLIP animation helpers for React",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "nwb build-react-component",
    "clean": "nwb clean-module && nwb clean-demo",
    "start": "nwb serve-react-demo",
    "test": "jest",
    "deploy-demo": "yarn run build; cp demo/dist/index.html demo/dist/200.html; surge demo/dist --domain literate-fly.surge.sh",
    "test:coverage": "nwb test-react --coverage",
    "test:watch": "nwb test-react --server",
    "preversion": "npm test",
    "version": "yarn run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aholachek/react-flip-toolkit"
  },
  "dependencies": {
    "prop-types": "^15.6.2",
    "rematrix": "^0.2.2",
    "shifty": "^2.3.1"
  },
  "peerDependencies": {
    "react": "16.x"
  },
  "devDependencies": {
    "babel-preset-env": "^1.7.0",
    "animejs": "^2.2.0",
    "babel-preset-react": "^6.24.1",
    "jest": "^23.1.0",
    "normalize.css": "^8.0.0",
    "nwb": "0.21.x",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-test-renderer": "^16.4.1"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "<rootDir>/jest.transform.js"
    },
    "transformIgnorePatterns": [
      "/!node_modules/shifty/"
    ]
  },
  "author": "Alex Holachek",
  "license": "MIT",
  "keywords": [
    "react-component",
    "FLIP",
    "transition",
    "animation"
  ]
}
